{
    "name": "machacekm/livewire-drawer",
    "description": "Drawer component for Laravel Livewire",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Machacek",
            "email": "machacekmartin@icloud.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Machacekm\\LivewireDrawer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Machacekm\\LivewireDrawer\\LivewireDrawerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 